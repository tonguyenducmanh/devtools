{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug td_tool_api",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/src_backend/td_app/cmd/api_app/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "GO_ENV": "development"
      },
      "showLog": true
    },
    {
      "name": "Debug td_tool_daemon",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/src_backend/td_app/cmd/daemon_app/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "GO_ENV": "development"
      },
      "showLog": true
    }
  ]
}
